      <mets:file ID="IMAGE_0003" MIMETYPE="image/tif"><mets:FLocat LOCTYPE="OTHER" xlink:href="file://0003.tif"/></mets:file>
      <mets:file ID="IMAGE_0004" MIMETYPE="image/tif"><mets:FLocat LOCTYPE="OTHER" xlink:href="file://0004.tif"/></mets:file>
      <mets:file ID="IMAGE_0005" MIMETYPE="image/tif"><mets:FLocat LOCTYPE="OTHER" xlink:href="file://0005.tif"/></mets:file>
    </mets:fileGrp>
  </mets:fileSec>
</mets:mets>
